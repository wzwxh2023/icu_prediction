{
  "version": 2,
  "builds": [
    {
      "src": "api/predict.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/predict",
      "dest": "/api/predict.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/predict.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "public": false
}
